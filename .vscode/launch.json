  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker Debug Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/install_x86/config/lib/config/ConfigNode_exec",
      "miDebuggerServerAddress": "172.18.0.10:1234",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": true,
      "preLaunchTask": "start_debug_container",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "linux": {
        "MIMode": "gdb"
      },
      "osx": {
        "MIMode": "gdb"
      },
      "windows": {
        "MIMode": "gdb"
      },
      "sourceFileMap": {
        "/opt/ws_kros/": "${workspaceRoot}/"
      }
    },
